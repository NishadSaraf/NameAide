<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.pdx.nishad.nameaidedemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:icon="@drawable/rounded_corners"
            android:label="Grad Walk"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayInfo"
            android:label="Grad Ceremony"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CheckQueue"
            android:label="Real-time Queue"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CheckStudData"
            android:label="Student Database"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AppInfo"
            android:label="Help"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>